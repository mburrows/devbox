source ~/.config/base16-shell/scripts/base16-tomorrow-night.sh

export EDITOR=vim
export PS1="\[\e[32m\]\u\[\e[m\]\[\e[32m\]:\[\e[m\]\[\e[35m\]\w\[\e[m\]\[\e[37m\]\\$\[\e[m\] "

alias rl='source ~/.bashrc'
alias g='git grep'
alias l='ls -lrt'
alias tmux='tmux -2'

# Start a fish shell if we have one
if [ -f /usr/bin/fish ] ; then
    exec /usr/bin/fish
fi
